{
    "version": "1.4",
    "configurations": [
        {
            "name": "default",
            "id": "",
            "device": "ATtiny1616",
            "imageType": "application",
            "imagePath": "./out/TelemetryPeripheral/default.elf",
            "toolProperties": "default-tool-base",
            "packs": [
                {
                    "name": "ATtiny_DFP",
                    "vendor": "Microchip",
                    "version": "3.2.268"
                }
            ],
            "fileSet": "default",
            "toolchain": "toolchain AVR-GCC 7.3.0"
        }
    ],
    "propertyGroups": [
        {
            "name": "toolchain AVR-GCC 7.3.0",
            "extend": "",
            "type": "toolchain",
            "provider": "microchip.toolchains:xc8@3.00",
            "properties": {
                "compiler++.preprocessor-macros": [
                    "__ATtiny1616__=\"hi!\""
                ]
            }
        },
        {
            "name": "default-AVR-GCC",
            "type": "toolchain",
            "provider": "microchip.toolchains:avr-gcc@7.3.0",
            "properties": {}
        },
        {
            "name": "default-tool-base",
            "type": "tool",
            "properties": {}
        }
    ],
    "fileSets": [
        {
            "name": "default",
            "files": [
                {
                    "include": "**/*",
                    "exclude": "**/(_build|out|cmake|.vscode)/**/*"
                }
            ]
        }
    ]
}
